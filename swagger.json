  "definitions": {
    "Concesionario": {
      "type": "object",
      "properties": {
        "nombre": {
          "type": "string"
        },
        "direccion": {
          "type": "string"
        },
        "listado": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Coche"
          }
        }
      },
      "required": ["nombre", "direccion"]
    },
    "Coche": {
      "type": "object",
      "properties": {
        "modelo": {
          "type": "string"
        },
        "precio": {
          "type": "integer"
        },
        "cv": {
          "type": "integer"
        }
      },
      "required": ["modelo", "precio", "cv"]
    }
  },
