        "parameters": [
          {
            "in": "body",
            "name": "concesionario",
            "schema": {
              "$ref": "#/definitions/Concesionario",
              "example": {
                "nombre": "Ferrari",
                "direccion": "Calle Retama nº 19",
                "listado": [
                  {
                    "modelo": "F40",
                    "precio": 1000000,
                    "cv": 478
                  }
                ]
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "{message: 'Concesionario añadido'}"
          }
        }
      }
    },
